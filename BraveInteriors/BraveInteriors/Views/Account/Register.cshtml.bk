        <td>&nbsp;</td>
        <td>&nbsp;</td>
        <td colspan="5"><input type="submit" name="button" id="button" value="Register" />
          <input type="submit" name="button2" id="button2" value="Cancel" /></td>
      </tr>
    </table>
} </div>
  <!-- End user-reg -->
</div>
