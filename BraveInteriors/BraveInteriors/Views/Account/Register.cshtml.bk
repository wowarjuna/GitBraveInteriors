<div id="page-body" style="position: relative; background-position: center; padding: 0px;
    margin: 0px; border: none; top: 134px; height: 534px; width: 1000px; overflow-x:hidden; overflow-y:auto;">
  <div style="width: 960px; margin-left: 8px; margin-top: 20px;"> <span class="Main-bold-blacktext">New to Brave Interiors?</span></div>
  <!-- Start user-reg -->
  <div class="user-reg-div"> @using (Html.BeginForm("Register", "Account"))
